    "type": "silverstripe-module",
    "keywords": [
        "silverstripe",
        "extension"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Werner KrauÃŸ",
            "email": "werner.krauss@netwerkstatt.at",
            "homepage": "http://www.netwerkstatt.at",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/wernerkrauss/silverstripe-folderperpage/issues"
    },
    "require": {
        "php": ">=7.0.0",
        "silverstripe/cms": "^4.0",
        "silverstripe/assets": "^1.0"
